{
  "name": "ATSocketMongo",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "inputField": "attachment_body",
      "outputFields": [
        {"name":"product"},
        {"name":"price"}
      ],
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "product",
                  "price"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    }
  ],
  "cubeProcess" : {
    "cubes": [
      {
        "name": "testCube",
        "checkpointConfig": {
          "timeDimension": "minute",
          "granularity": "minute",
          "interval": 30000,
          "timeAvailability": 60000
        },
        "dimensions": [
          {
            "name": "product",
            "field": "product"
          }
        ],
        "operators": [
          {
            "name": "sum-operator",
            "type": "Sum",
            "configuration": {
              "inputField": "price"
            }
          },
          {
            "name": "avg-operator",
            "type": "Mean",
            "configuration": {
              "inputField": "price"
            }
          }
        ]
      }
    ]
  },
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": "localhost:60000",
        "dbName": "csvtest"
      }
    }
  ]
}